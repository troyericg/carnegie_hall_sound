<% @events.each do |evnt| %>
	<div id="<%= evnt.id %>" class="gallery-img-container group" attr-presenter="<%= evnt.presenter %>" attr-title="<%= evnt.title %>" attr-date="<%= evnt.correct_date %>" attr-audioID="<%= evnt.correct_audio_id %>">
		<%= image_tag(evnt.correct_img_url) %>
	</div>
<% end %>
