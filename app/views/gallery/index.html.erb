<% @events.each do |evnt| %>
	<div id="<%= evnt.id %>" class="gallery-img-container group" attr-presenter="<%= evnt.presenter %>" attr-title="<%= evnt.title %>" attr-date="<%= correct_date(evnt.date) %>" attr-loc="<%= evnt.location %>" attr-audioID="<%= evnt.audio_id %>">
		<%= image_tag(evnt.img_url) %>
	</div>
<% end %>
